{
    "manifest_version": 2,
  
    "name": "Sitecore Content Tree Alphabetizer",
    "description": "Temporarily alphabetizes the visible Sitecore Content Tree.",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "sitecore-tree-alphabetizer.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }